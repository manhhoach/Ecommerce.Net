@model List<Category>

<h1>Category list</h1>

<table class="table table-bordered table-striped">
    <tr>
        <th>Name</th>
        <th>Display Order</th>
    </tr>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            </tr>
        }

    </tbody>
</table>